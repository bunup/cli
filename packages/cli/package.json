{
	"name": "@bunup/cli",
	"description": "Bunup's CLI that does the heavy lifting.",
	"version": "0.0.7",
	"type": "module",
	"files": ["dist"],
	"module": "./dist/index.js",
	"main": "./dist/index.cjs",
	"types": "./dist/index.d.ts",
	"scripts": {
		"publish:ci": "bun publish --access public --no-git-checks"
	},
	"bin": {
		"bunup-cli": "./dist/index.js"
	},
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bunup/bunup-kit.git"
	},
	"homepage": "https://github.com/bunup/bunup-kit/tree/main/packages/create#readme",
	"dependencies": {
		"@clack/prompts": "^0.11.0",
		"giget": "^2.0.0",
		"picocolors": "^1.1.1",
		"replace-in-file": "^8.3.0",
		"tinyexec": "^1.0.1",
		"zlye": "^0.1.5"
	}
}
